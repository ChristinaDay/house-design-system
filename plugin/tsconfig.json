{
  "compilerOptions": {
    "target": "es2019",
    "lib": ["es2019"],
    "module": "commonjs",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": ".",
    "types": ["@figma/plugin-typings"],
    "noImplicitAny": false
  },
  "include": ["code.ts"],
  "exclude": ["node_modules"]
}
