{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["es2017"],
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": ".",
    "types": ["@figma/plugin-typings"]
  },
  "include": ["code.ts"],
  "exclude": ["node_modules"]
}
